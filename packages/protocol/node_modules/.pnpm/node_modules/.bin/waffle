#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/ethereum-waffle@3.4.4_typescript@5.2.2/node_modules/ethereum-waffle/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/ethereum-waffle@3.4.4_typescript@5.2.2/node_modules/ethereum-waffle/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/ethereum-waffle@3.4.4_typescript@5.2.2/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/ethereum-waffle@3.4.4_typescript@5.2.2/node_modules/ethereum-waffle/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/ethereum-waffle@3.4.4_typescript@5.2.2/node_modules/ethereum-waffle/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/ethereum-waffle@3.4.4_typescript@5.2.2/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ethereum-waffle/bin/waffle" "$@"
else
  exec node  "$basedir/../ethereum-waffle/bin/waffle" "$@"
fi
