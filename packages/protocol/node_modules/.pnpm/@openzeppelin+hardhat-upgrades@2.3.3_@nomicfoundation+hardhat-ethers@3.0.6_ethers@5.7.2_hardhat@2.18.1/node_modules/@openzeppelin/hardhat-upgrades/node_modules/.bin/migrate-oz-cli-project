else
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.3.3_@nomicfoundation+hardhat-ethers@3.0.6_ethers@5.7.2_hardhat@2.18.1/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.3.3_@nomicfoundation+hardhat-ethers@3.0.6_ethers@5.7.2_hardhat@2.18.1/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.3.3_@nomicfoundation+hardhat-ethers@3.0.6_ethers@5.7.2_hardhat@2.18.1/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.3.3_@nomicfoundation+hardhat-ethers@3.0.6_ethers@5.7.2_hardhat@2.18.1/node_modules/@openzeppelin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.3.3_@nomicfoundation+hardhat-ethers@3.0.6_ethers@5.7.2_hardhat@2.18.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
