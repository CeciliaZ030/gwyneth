#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/github.com+taikoxyz+solidity-coverage@ceb49fd1f6041e4fcd26079dfb0d3b0f58c812e5_hardhat@2.18.1/node_modules/solidity-coverage/plugins/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/github.com+taikoxyz+solidity-coverage@ceb49fd1f6041e4fcd26079dfb0d3b0f58c812e5_hardhat@2.18.1/node_modules/solidity-coverage/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/github.com+taikoxyz+solidity-coverage@ceb49fd1f6041e4fcd26079dfb0d3b0f58c812e5_hardhat@2.18.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/github.com+taikoxyz+solidity-coverage@ceb49fd1f6041e4fcd26079dfb0d3b0f58c812e5_hardhat@2.18.1/node_modules/solidity-coverage/plugins/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/github.com+taikoxyz+solidity-coverage@ceb49fd1f6041e4fcd26079dfb0d3b0f58c812e5_hardhat@2.18.1/node_modules/solidity-coverage/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/github.com+taikoxyz+solidity-coverage@ceb49fd1f6041e4fcd26079dfb0d3b0f58c812e5_hardhat@2.18.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
fi
